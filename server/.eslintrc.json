{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "project": "./tsconfig.json"
    },
    "rules": {
        "prettier/prettier": "error"
    },
    "plugins": ["prettier"]
}
